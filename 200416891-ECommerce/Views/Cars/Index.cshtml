@model List<_200416891_ECommerce.Models.Car>

@{
    ViewBag.Title = "Cars List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Cars List</h2>



    <table class="table">
        <tr>
            <th>
                ID
            </th>
            <th>
                Brand
            </th>
            <th>
                Model
            </th>
            <th>
                Type
            </th>
            <th>
                Year
            </th>
            <th></th>
        </tr>

        @foreach (var car in Model)
        {
            <tr>
                <td>
                    @car.Id
                </td>
                <td>
                    @car.Brand
                </td>
                <td>
                    @car.Model
                </td>
                <td>
                    @car.Type
                </td>
                <td>
                    @car.Year
                </td>
            </tr>
        }

    </table>